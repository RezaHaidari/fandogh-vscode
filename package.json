{
    "name": "fandogh-vscode",
    "displayName": "fandogh",
    "description": "fandogh vscode extension",
    "version": "0.0.5",
    "publisher": "rezahaidari",
    "repository": "https://github.com/RezaHaidari/fandogh-vscode",
    "icon": "icon.png",    
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.login",
        "onCommand:extension.deploy",
        "onCommand:extension.create_image",
        "onCommand:extension.create_version",
        "onCommand:extension.create_service",
        "onCommand:extension.open_file"
    ],
    "main": "./lib/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.login",
                "title": "fandogh login"
            },
            {
                "command": "extension.deploy",
                "title": "fandogh deploy"
            },
            {
                "command": "extension.create_image",
                "title": "fandogh create image"
            },
            {
                "command": "extension.create_version",
                "title": "fandogh build version"
            },
            {
                "command": "extension.create_service",
                "title": "fandogh run service"
            },
            {
                "command": "extension.open_file",
                "title": "fandogh open file"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "fandogh": "0.5.0"
    }
}
